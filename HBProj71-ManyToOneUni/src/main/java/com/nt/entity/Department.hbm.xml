<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.nt.entity.Department"   table="DEPARTMENT" >
       <id name="deptNo"   type="int" length="10" >
          <generator class="sequence">
              <param name="sequence_name">DEPTNO_SEQ</param>
          </generator>
       </id>
       <property name="dname"   type="string" length="10"/>
       <property name="location"   type="string" length="10"/>
       
  </class>

</hibernate-mapping>	